<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.protectdev.manage.mapper.TemplateMapper">
    <resultMap id="Template" type="com.protectdev.manage.pojo.Template">
        <id column="id" property="id"></id>
        <result column="tem_name" property="temName"></result>
        <result column="dev_id" property="devId"></result>
    </resultMap>

    <insert id="addTemplate" parameterType="com.protectdev.manage.pojo.Template">
        insert into template
        set id = #{id} , tem_name = #{temName} , dev_id = #{devId}
    </insert>

    <delete id="deleteTemplate">
        delete from template
        where id = #{temId}
    </delete>



    <select id="getTemplate">
        select *
        from template
        where id = #{id}
    </select>

    <update id="postTemplate">
        update template
        set tem_name = #{temName}
        where id = #{id}
    </update>



</mapper>